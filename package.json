    "react-dom": "^18"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.8.2",
    "@types/file-saver": "2.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-next-on-pages": "1.8.2",
